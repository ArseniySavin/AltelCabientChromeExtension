{
  "manifest_version": 2,

  "name": "Altel Cabinet",
  "description": "",
  "version": "1.0",
  "icons": {
    "16": "4g.png",
    "48": "4g.png",
    "128": "4g.png"
  },
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Altel traffic details"
  },

  "options_page": "options.html",

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "permissions": [ "https://cabinet.altel.kz/*", "fontSettings" , "activeTab", "tabs", "storage"],
  
  "background": {
    "scripts": ["script/background.js"]
  }
}
